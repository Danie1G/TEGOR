<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_picking_form_extension" model="ir.ui.view">
        <field name="name">stock.extension.form</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form" />
        <field name="arch" type="xml">

            <xpath expr="//header" position="inside">
                <button name="action_open_date_wizard" string="Seleccionar Fecha" class="oe_highlight"/>
            </xpath>
            

            <field name="origin" position="after">
                <!-- <field name="date_entry_check" attrs="{'invisible': [('picking_type_code', '!=', 'incoming')]}"/> -->
                <field name="date_entry" attrs="{'invisible': [('date_entry_check', '=', False)],  'readonly': True"/>

            </field>

        </field>
    </record>


</odoo>